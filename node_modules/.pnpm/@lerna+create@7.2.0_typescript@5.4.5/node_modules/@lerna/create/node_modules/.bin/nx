#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules/nx/bin/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules/nx/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules/nx/bin/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules/nx/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/nx@16.10.0/node_modules:/Users/mac/Desktop/Project/2025/TinyTest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nx@16.10.0/node_modules/nx/bin/nx.js" "$@"
else
  exec node  "$basedir/../../../../../../nx@16.10.0/node_modules/nx/bin/nx.js" "$@"
fi
